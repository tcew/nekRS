#pragma unroll
	    for (int l = 0; l < p_Nq_e; l++)
	      value += S_y_e[es][j][l] * work1[es][k][l][i];
	    work2[es][k][j][i] = value;
	  }
	}
      }
  
    @barrier();
    for(int es = 0; es<p_Nblk; ++es; @inner)
      for (int j = 0; j < p_Nq_e; j++; @inner) {
