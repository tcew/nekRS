	    S_y_e[es][i][j] = S_y[ij];
	    S_z_e[es][i][j] = S_z[ij];
	    S_x_eT[es][j][i] = S_x_e[es][i][j];
	    S_y_eT[es][j][i] = S_y_e[es][i][j];
	    S_z_eT[es][j][i] = S_z_e[es][i][j];
	  }
	}
