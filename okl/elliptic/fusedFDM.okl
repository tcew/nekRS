    @barrier();
    for (int k = 0; k < p_Nq_e; k++; @inner) {
      for (int j = 0; j < p_Nq_e; j++; @inner) {

#pragma nounroll
        for (int i = 0; i < p_Nq_e; i+=2) {
          pfloat value1 = 0.0;
	  pfloat value2 = 0.0;
	  //#pragma unroll p_Nq_e
#pragma nounroll
          for (int l = 0; l < p_Nq_e; l++){
	    pfloat S_y_eT_jl = S_y_eT[j][l];
