    for(int k = 0; k < p_Nq_e; ++k; @inner){
      for(int j = 0; j < p_Nq_e; ++j; @inner){
        if(k >= 1 && k < p_Nq_e - 1 && j >= 1 && j < p_Nq_e - 1) {
          const int l1 = 0;
          const int l2 = 2;
          work1[l1][j][k] = work1[l1][j][k] - work1[l2][j][k];
          work1[p_Nq_e - l1 - 1][j][k] = work1[p_Nq_e - l1 - 1][j][k] -
	    work1[p_Nq_e - l2 - 1][j][k];
