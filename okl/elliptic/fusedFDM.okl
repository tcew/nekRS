	    work2[es][i][l1][j] = work1[es][i][l2][j];
	    work2[es][i][p_Nq_e - l1 - 1][j] = work1[es][i][p_Nq_e - l2 - 1][j];

	    work2[es][i][j][l1] = work1[es][i][j][l2];
