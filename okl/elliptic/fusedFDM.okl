            value1 += S_x_eT_jl * work1[k][i][l];
	    value2 += (i+1<p_Nq_e) ? S_x_eT_jl * work1[k][i+1][l]: 0;
	  }
          work2[k][j][i] = value1;
	  if(i+1<p_Nq_e)
