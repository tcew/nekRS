    @shared pfloat S_z_eT[p_Nblk][p_Nq_e][p_Nq_e];
    @shared pfloat work1[p_Nblk][p_Nq_e][p_Nq_e][p_Nq_e];
    @shared pfloat work2[p_Nblk][p_Nq_e][p_Nq_e][p_Nq_e];
